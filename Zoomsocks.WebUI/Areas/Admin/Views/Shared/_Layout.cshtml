<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Zoomsocks Admin</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/frontend/admin/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href="~/frontend/admin/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/frontend/admin/css/_all-skins.css" rel="stylesheet" />
    <link href="~/frontend/admin/css/datepicker3.css" rel="stylesheet" />
    <link href="~/frontend/admin/css/daterangepicker-bs3.css" rel="stylesheet" />
    <link href="~/frontend/admin/css/bootstrap3-wysihtml5.min.css" rel="stylesheet" />
    <link href="~/frontend/admin/css/toastr.css" rel="stylesheet" />
    <link href="~/frontend/admin/css/style.css" rel="stylesheet" />
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <a href="zoomsocks-admin" class="logo">
                <span class="logo-mini"><b>A</b>LT</span>
                <span class="logo-lg"><b>Zoomsocks</b></span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    <li class="treeview">
                        @Html.ActionLink("Product Categories", "List", "ProductCategory", new { Area = "Admin" }, null)
                    </li>
                    <li class="treeview">
                        @Html.ActionLink("Products", "List", "Product", new { Area = "Admin" }, null)
                    </li>
                </ul>
            </section>
        </aside>

        <div class="content-wrapper">
            <section class="content">
                @RenderBody()
            </section>
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.3.0
            </div>
            <strong>Copyright &copy; <a href="#">Zoomsocks</a>.</strong> All rights reserved.
        </footer>
    </div>

    <script src="~/frontend/common/js/jquery-3.2.1.min.js"></script>
    <script src="~/frontend/admin/js/bootstrap.min.js"></script>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>    
    <script src="~/frontend/common/js/fontawesome.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/frontend/admin/js/toastr.min.js"></script>
    <script src="~/frontend/admin/js/mustache.min.js"></script>
    <script src="~/frontend/common/js/moment.min.js"></script>
    <script src="~/frontend/admin/js/daterangepicker.js"></script>
    <script src="~/frontend/admin/js/bootstrap-datepicker.js"></script>
    <script src="~/frontend/admin/js/bootstrap3-wysihtml5.all.min.js"></script>
    <script src="~/frontend/admin/js/app.min.js"></script>
    <script>$.widget.bridge('uibutton', $.ui.button);</script>
    @RenderSection("js", false)
</body>
</html>
